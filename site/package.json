{
  "name": "osprey",
  "description": "Svelte/Sapper Website Template",
  "version": "0.0.1",
  "scripts": {
    "eslint": "eslint",
    "eslint:fix": "eslint --fix ./*.js ./src/*.js ./src/routes/**/*.svelte ./src/components/**/*.svelte",
    "dev": "sapper dev",
    "build": "cross-env NODE_ENV=production sapper build --legacy",
    "prod": "npm run build",
    "export": "cross-env NODE_ENV=production sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "@sanity/client": "^2.0.1",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "polka": "next",
    "sapper-environment": "^1.0.1",
    "sirv": "^1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "@beyonk/svelte-carousel": "^2.8.0",
    "@movingbrands/svelte-portable-text": "^0.0.11",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@sanity/block-content-to-hyperscript": "^2.0.10",
    "@sanity/image-url": "^0.140.19",
    "@tailwindcss/typography": "^0.2.0",
    "@tailwindcss/ui": "^0.6.0",
    "chroma-js": "^2.1.0",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "get-youtube-id": "^1.0.1",
    "kleur": "^4.1.3",
    "postcss": "^8.1.1",
    "postcss-import": "^12.0.1",
    "rollup": "^2.31.0",
    "rollup-plugin-svelte": "^6.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.28.10",
    "svelte": "^3.29.0",
    "svelte-highlight": "^0.6.2",
