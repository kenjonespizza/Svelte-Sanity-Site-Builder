{
  "name": "osprey",
  "description": "Svelte/Sapper Website Template",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "watch:tailwind": "postcss static/tailwind.css -o static/index.css -w",
    "build:tailwind": "NODE_ENV=production postcss static/tailwind.css -o static/index.css",
    "go": "concurrently \"npm run dev\" \"npm run watch:tailwind\""
  },
  "dependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@sanity/client": "^1.150.7",
    "compression": "^1.7.1",
    "polka": "next",
    "postcss-cli": "^7.1.2",
    "sirv": "^1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.0.0",
    "@beyonk/svelte-carousel": "^2.8.0",
    "@movingbrands/svelte-portable-text": "^0.0.11",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@sanity/block-content-to-hyperscript": "^2.0.10",
    "@sanity/image-url": "^0.140.19",
    "@tailwindcss/typography": "^0.2.0",
    "@tailwindcss/ui": "^0.6.0",
    "chroma-js": "^2.1.0",
    "concurrently": "^5.2.0",
    "get-youtube-id": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.26.11",
    "rollup-plugin-svelte": "^6.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.28.6",
    "svelte": "^3.25.0",
    "svelte-local-storage-store": "^0.1.3",
    "svelte-youtube": "^0.0.1",
    "tailwindcss": "^1.8.9"
  }
}
